{
  "name": "marquinhos-web-api",
  "version": "1.0.0",
  "description": "Marquinhos API",
  "main": "dist",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "express": "^4.18.2",
    "mongodb": "^5.1.0",
    "mongoose": "^7.4.3",
    "morgan": "^1.10.0",
    "tslib": "^2.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.17.5",
    "typescript": "5.1.6"
  },
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p tsconfig.json",
